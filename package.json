{
	"name": "mathquill",
	"description": "Easily type math in your webapp",
	"version": "0.10.1",
	"license": "MPL-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/mathquill/mathquill.git"
	},
	"main": "lib/publicapi.js",
	"types": "lib/publicapi.d.ts",
	"files": [
		"lib/**/*",
		"dist/**/*"
	],
	"scripts": {
		"build-lib": "tsc --project tsconfig-lib.json && tsconfig-replace-paths -p tsconfig-lib.json -s ./src -o ./lib",
		"devbuild": "webpack --mode development",
		"build": "webpack --mode production",
		"build-dist": "webpack --mode production --env outputPath=dist",
		"build-all-js": "BUILD_BASIC=true webpack --mode production",
		"serve": "webpack serve --mode development",
		"jslint": "eslint --ext .js,.ts ./src ./test ./public --fix",
		"csslint": "stylelint \"./src/**/*.less\" \"./public/**/*.css\" --fix",
		"lint": "npm run jslint && npm run csslint"
	},
	"devDependencies": {
		"@babel/cli": "^7.15.7",
		"@babel/core": "^7.15.8",
		"@babel/eslint-parser": "^7.15.8",
		"@babel/preset-env": "^7.15.8",
		"@typescript-eslint/eslint-plugin": "^5.2.0",
		"@typescript-eslint/parser": "^5.2.0",
		"babel-loader": "^8.2.2",
		"copy-webpack-plugin": "^9.0.1",
		"css-loader": "^6.4.0",
		"css-minimizer-webpack-plugin": "^3.1.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-webpack-plugin": "^3.0.1",
		"less": ">=1.5.1",
		"less-loader": "^10.1.0",
		"mini-css-extract-plugin": "^2.4.2",
		"mocha": "^9.1.3",
		"stylelint": "^13.13.1",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard": "^22.0.0",
		"stylelint-webpack-plugin": "^3.0.1",
		"ts-loader": "^9.2.6",
		"tsconfig-replace-paths": "^0.0.11",
		"typescript": "^4.4.4",
		"webpack": "^5.55.0",
		"webpack-cli": "^4.8.0",
		"webpack-dev-server": "^4.7.4"
	},
	"browserslist": [
		"last 10 Chrome versions",
		"last 10 Firefox versions",
		"last 4 Edge versions",
		"last 7 Safari versions",
		"last 8 Android versions",
		"last 8 ChromeAndroid versions",
		"last 8 FirefoxAndroid versions",
		"last 10 iOS versions",
		"last 5 Opera versions"
	]
}
