{
	"name": "@openwebwork/mathquill",
	"description": "Easily type math in your webapp",
	"version": "0.11.0-beta.1",
	"license": "MPL-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/openwebwork/mathquill.git"
	},
	"homepage": "https://github.com/openwebwork/mathquill",
	"main": "lib/publicapi.js",
	"types": "lib/publicapi.d.ts",
	"files": [
		"lib/**/*",
		"dist/**/*"
	],
	"scripts": {
		"build:lib": "tsc --project tsconfig-lib.json && tsconfig-replace-paths -p tsconfig-lib.json -s ./src -o ./lib",
		"build:dev": "webpack --mode development",
		"build:all-js": "BUILD_BASIC=true webpack --mode production",
		"build": "webpack --mode production",
		"serve": "webpack serve --mode development",
		"lint:eslint": "eslint ./src ./test ./public --fix",
		"lint:stylelint": "stylelint \"./src/**/*.less\" \"./public/**/*.css\" \"./public/**/*.html\" --fix",
		"lint": "npm run lint:eslint && npm run lint:stylelint",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@awmottaz/prettier-plugin-void-html": "^1.6.1",
		"@stylistic/eslint-plugin": "^2.7.2",
		"copy-webpack-plugin": "^12.0.2",
		"css-loader": "^7.1.2",
		"css-minimizer-webpack-plugin": "^7.0.0",
		"eslint": "^9.9.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-webpack-plugin": "^4.2.0",
		"globals": "^15.9.0",
		"less": "^4.2.0",
		"less-loader": "^12.2.0",
		"mini-css-extract-plugin": "^2.9.1",
		"mocha": "^10.7.3",
		"postcss-less": "^6.0.0",
		"prettier": "^3.3.3",
		"stylelint": "^16.9.0",
		"stylelint-config-html": "^1.1.0",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-webpack-plugin": "^5.0.1",
		"ts-loader": "^9.5.1",
		"tsconfig-replace-paths": "^0.0.14",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.4.0",
		"webpack": "^5.94.0",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^5.1.0"
	},
	"browserslist": [
		"last 10 Chrome versions",
		"last 10 Firefox versions",
		"last 4 Edge versions",
		"last 7 Safari versions",
		"last 8 Android versions",
		"last 8 ChromeAndroid versions",
		"last 8 FirefoxAndroid versions",
		"last 10 iOS versions",
		"last 5 Opera versions"
	]
}
